<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apppruebaatlantico">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="RestauranteWorldSkills"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppPruebaAtlantico">
        <activity
            android:name=".MostrarProductos"
            android:exported="true"
            android:theme="@style/Theme.ActionBar"/>
        <activity
            android:name=".Home"
            android:exported="true"
            android:theme="@style/Theme.ActionBar" />
        <activity
            android:name=".especialidadCasa"
            android:exported="true"
            android:theme="@style/Theme.NoactionBar" />
        <activity
            android:name=".condicionesuser"
            android:exported="true"
            android:theme="@style/Theme.NoactionBar" />
        <activity
            android:name=".Registrar"
            android:exported="true"
            android:theme="@style/Theme.NoactionBar" />
        <activity
            android:name=".Login"
            android:exported="true"
            android:theme="@style/Theme.NoactionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.NoactionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>